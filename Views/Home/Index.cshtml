@model IEnumerable<Talent>

@{ 
    ViewBag.Title = "Talent List";
}


<div class="card-deck">
    @foreach (var talent in @Model)
    {
        <div class="card m-3">
            <div class="card-header">
                <h3>@talent.Name</h3>
            </div>
            <img class="card-img-top" src="~/images/download.jpg" asp-append-version="true"/>
            <div class="card-footer text-center">
                <a asp-controller="home" asp-action="details" asp-route-id="@talent.Id" class="btn btn-primary m-1">View</a>
                <a href="#" class="btn btn-primary m-1">Edit</a>
                <a href="#" class="btn btn-danger m-1">Delete</a>
            </div>
        </div>
    }
</div>